{
    "name": "gridplus-takehome",
    "version": "1.0.0",
    "description": "Takehome test for Grid+.",
    "main": "_",
    "private": true,
    "scripts": {
        "build": "rm -rf public && webpack",
        "build:dev": "rm -rf public && webpack --cache --config webpack.dev.config.js",
        "lint": "tslint --fix src/**/*.{ts,tsx} & stylelint --cache --fix src/**/*.css",
        "precommit": "npm run test && npm run lint",
        "start:dev": "cross-env NODE_ENV=development nodemon src/server.dev.ts",
        "test": "jest --config=jest.config.json",
        "test:dev": "jest --config=jest.config.json --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GridPlus/john-pittman-takehome.git"
    },
