{
    "rootDir": "./",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
    "moduleNameMapper": {
        "\\.(css)$": "react"
    },
    "projects": ["<rootDir>/src"],
    "setupTestFrameworkScriptFile": "./enzyme.config.ts",
    "transform": {
        "\\.js$|\\.jsx$|\\.ts$|\\.tsx$": "ts-jest"
    },
    "testRegex": ".\\.test\\.(js?|jsx?|ts?|tsx?)$"
}
