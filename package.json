    "bugs": {
        "url": "https://github.com/GridPlus/john-pittman-takehome/issues"
    },
    "homepage": "https://github.com/GridPlus/john-pittman-takehome#readme",
    "dependencies": {
        "awesome-typescript-loader": "^5.0.0",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "express": "^4.16.3",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.0",
        "optimize-css-assets-webpack-plugin": "^4.0.0",
        "postcss-loader": "^2.1.3",
        "postcss-nested": "^3.0.0",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-redux": "^5.0.7",
