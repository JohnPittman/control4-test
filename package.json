        "@types/webpack-env": "^1.13.5",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "jest": "^22.4.3",
        "nodemon": "^1.17.3",
        "react-hot-loader": "^4.0.1",
        "stylelint": "^8.0.0",
        "stylelint-config-property-sort-order-smacss": "^3.0.1",
