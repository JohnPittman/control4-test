{
    "name": "gridplus-takehome",
    "version": "1.0.0",
    "description": "Takehome test for Grid+.",
    "main": "_",
    "private": true,
    "scripts": {
        "build": "cross-env NODE_ENV=production ts-node fuse.ts build",
        "build:dev": "cross-env NODE_ENV=development ts-node fuse.ts build:dev",
        "clean": "rm -rf public .fusebox",
        "lint": "tslint --fix src/**/*.{ts,tsx} & stylelint --cache --fix src/**/*.css",
        "precommit": "npm run test && npm run lint",
        "start:dev": "cross-env NODE_ENV=development nodemon fuse.ts start:dev",
        "start-docker:dev": "cross-env NODE_ENV=development nodemon fuse.ts start-docker:dev",
        "test": "jest --config=jest.config.json -o",
        "test:watch": "jest --config=jest.config.json --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GridPlus/john-pittman-takehome.git"
    },
    "author": "John Pittman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GridPlus/john-pittman-takehome/issues"
    },
    "homepage": "https://github.com/GridPlus/john-pittman-takehome#readme",
    "dependencies": {
        "cross-env": "^5.1.4",
        "express": "^4.16.3",
        "fuse-box": "^3.2.1",
        "postcss": "^6.0.21",
        "postcss-nested": "^3.0.0",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "ts-node": "^5.0.1",
        "typescript": "^2.8.1",
        "uglify-es": "^3.3.9"
    },
    "devDependencies": {
        "@types/jest": "^22.2.2",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "jest": "^22.4.3",
        "nodemon": "^1.17.3",
        "prettier": "^1.12.0",
        "stylelint": "^8.0.0",
        "stylelint-config-property-sort-order-smacss": "^3.0.1",
        "ts-jest": "^22.4.2",
        "tslint": "^5.9.1",
        "tslint-config-prettier": "^1.10.0",
        "tslint-plugin-prettier": "^1.3.0",
        "tslint-react": "^3.5.1"
    }
}
